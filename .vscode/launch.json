  "version": "0.2.0",
  "configurations": [
    {
      "name": "Parallel Tester debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/elektra_test",
      "args": ["--gtest_break_on_failure", "--gtest_catch_exceptions=0"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "preLaunchTask": "build"
    },
    {
      "name": "Parallel - No Build",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/elektra_test",
      "args": ["--gtest_break_on_failure", "--gtest_catch_exceptions=0"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "Serial Tester debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/elektra_test",
      "args": ["--gtest_break_on_failure", "--gtest_catch_exceptions=0"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "preLaunchTask": "debugBuild"
    },
    {
      "name": "Serial Tester - no build",
      "type": "cppdbg",
      "request": "launch",
      "targetArchitecture": "arm",
      "program": "${workspaceFolder}/build/elektra_test",
      "args": ["--gtest_break_on_failure", "--gtest_catch_exceptions=0"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
    },
    {
      "name": "Benchmark debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/elektra",
      "args": ["../benchmarks/graphs/basic.txt"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "preLaunchTask": "debugBuild"
    }
  ]
}
